<AnchorPane xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.PTCODE.virtualgarden.view.ViewGardensController">
   <children>
      <ImageView fitHeight="1085.0" fitWidth="1788.0" layoutX="133.0" layoutY="-3.0" opacity="0.05" pickOnBounds="true">
         <image>
            <Image url="@../image/LogoGarden.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="1086.0" fitWidth="146.0" layoutX="-5.0" layoutY="-2.0" pickOnBounds="true">
         <image>
            <Image url="@../image/color.png" />
         </image>
      </ImageView>
      <ScrollPane layoutX="197.0" layoutY="235.0" prefHeight="728.0" prefWidth="1686.0" style="-fx-border-radius: 10;">
         <content>
            <VBox prefHeight="730.0" prefWidth="1688.0" fx:id="gardensContainer">

            </VBox>
         </content>
      </ScrollPane>
      <Label alignment="CENTER" layoutX="683.0" layoutY="57.0" prefHeight="97.0" prefWidth="748.0" text="View all your gardens">
         <font>
            <Font name="System Bold" size="66.0" />
         </font>
      </Label>
      <ImageView fitHeight="102.0" fitWidth="106.0" layoutX="13.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/seta.png" />
         </image>
         <effect>
            <Lighting diffuseConstant="0.98" specularConstant="2.0" specularExponent="0.0" surfaceScale="0.0">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
      </ImageView>
      <Button layoutX="21.0" layoutY="18.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="105.0" prefWidth="88.0" style="-fx-background-color: transparent;">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
